  "userdoc": {
    "methods": {
      "approve(address,uint256)": {
        "notice": "Set allowance for other address\r      * Allows `_spender` to spend no more than `_value` tokens in your behalf\r "
      },
      "burn(uint256)": {
        "notice": "Destroy tokens\r      * Remove `_value` tokens from the system irreversibly\r "
      },
      "buy(uint256)": {
        "notice": "Buy tokens\r"
      },
      "getBuyPrice()": {
        "notice": "Get token price\r"
      },
      "transferFrom(address,address,uint256)": {
        "notice": "Transfer tokens from other address\r      * Send `_value` tokens to `_to` in behalf of `_from`\r "
      }
    }
